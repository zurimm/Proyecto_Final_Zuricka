@model Eventos.Models.Soporte

@{
    ViewData["Title"] = "Edit";
}
<link href="~/css/estiloadmin.css" rel="stylesheet" />
<div id="estilo">
    <div style="margin-left:5%; margin-top:10%">
        <h1>Editar</h1>

        <h4>Soporte</h4>
        <hr />
        <div class="row">
            <div class="col-md-4">
                <form asp-action="Edit">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="Id" />
                    <div class="form-group">
                        <label asp-for="Nombre_Soporte" class="control-label"></label>
                        <input asp-for="Nombre_Soporte" class="form-control" />
                        <span asp-validation-for="Nombre_Soporte" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="email_Soporte" class="control-label"></label>
                        <input asp-for="email_Soporte" class="form-control" />
                        <span asp-validation-for="email_Soporte" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="contraseña_Soporte" class="control-label"></label>
                        <input asp-for="contraseña_Soporte" class="form-control" />
                        <span asp-validation-for="contraseña_Soporte" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Save" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>

        <div>
            <a asp-action="Index">Back to List</a>
        </div>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
